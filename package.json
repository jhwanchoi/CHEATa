{
    "name": "cheata",
    "displayName": "Cheata",
    "version": "0.0.1",
    "description": "cheata is a fake information detection chrome extension",
    "author": "jhwan333@gmail.com",
    "scripts": {
        "dev": "plasmo dev",
        "build": "plasmo build",
        "package": "plasmo package",
        "lint": "eslint src/**/*.{ts,tsx}",
        "lint:fix": "eslint --fix src/**/*.{ts,tsx}",
        "format": "prettier --write src/**/*.{ts,tsx}"
    },
    "dependencies": {
        "plasmo": "0.90.3",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@ianvs/prettier-plugin-sort-imports": "4.1.1",
        "@types/chrome": "0.0.258",
        "@types/node": "20.11.5",
        "@types/react": "18.2.48",
        "@types/react-dom": "18.2.18",
        "@typescript-eslint/eslint-plugin": "^6.19.0",
        "@typescript-eslint/parser": "^6.19.0",
        "autoprefixer": "^10.4.16",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.33.2",
        "postcss": "^8.4.33",
        "prettier": "3.2.4",
        "tailwindcss": "^3.4.1",
        "typescript": "5.3.3"
    },
    "manifest": {
        "host_permissions": [
            "https://*/*"
        ],
        "permissions": [
            "storage",
            "notifications"
        ]
    }
}